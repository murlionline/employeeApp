<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ns.EmpModule.controller.PersonalData" displayBlock="true">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="Employee Inf">
					<content>
					    <List noDataText="Drop list items here" id="list0" items="{/EmployeeCollection}">
					        <items>
					            <StandardListItem type="Navigation" title="{employeeID}" description="{fullName}" icon="sap-icon://picture" id="item0"/>
					        </items>
					    </List>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>